{
  "name": "breader",
  "version": "2.0.0",
  "private": true,
  "description": "A simple RSS reader.",
  "homepage": "https://github.com/breeze2/breader",
  "author": {
    "email": "805511939@qq.com",
    "name": "breezelin",
    "url": "https://github.com/breeze2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/breeze2/breader.git"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "public/electron.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "prebuild": "yarn build:main",
    "preelectron-dev": "yarn build:main",
    "preelectron-dist": "yarn build",
    "electron-dev": "cross-env BROWSER=none concurrently \"yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-dist": "electron-builder build",
    "electron-pack": "electron-builder build",
    "electron-start": "electron .",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build:main": "yarn webpack --config ./main/webpack.config.js",
    "lint": "tslint src/**/*.ts src/**/*.tsx main/**/*.ts -e src/serviceWorker.ts",
    "test": "react-app-rewired test",
    "test:coverage": "react-app-rewired test --coverage",
    "eject": "react-scripts eject",
    "commit": "git cz",
    "commit:amend": "git commit --amend --no-edit",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn check --integrity && pretty-quick --staged && yarn lint && rm -f ./public/electron.js*",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@sentry/electron": "^0.17.3",
    "chardet": "^0.8.0",
    "custom-electron-titlebar": "^3.1.0",
    "electron-is-dev": "^1.0.1",
    "electron-updater": "^4.1.2",
    "feedparser": "^2.2.9",
    "iconv-lite": "^0.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@jest-runner/electron": "^2.0.2",
    "@sentry/cli": "^1.47.1",
    "@types/chardet": "^0.5.0",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/feedparser": "^2.2.3",
    "@types/jest": "24.0.17",
    "@types/node": "12.7.2",
    "@types/pouchdb-adapter-memory": "^6.1.3",
    "@types/pouchdb-browser": "^6.1.3",
    "@types/pouchdb-find": "^6.3.4",
    "@types/react": "16.9.2",
    "@types/react-custom-scrollbars": "^4.0.6",
    "@types/react-dom": "16.8.5",
    "@types/react-redux": "^7.0.1",
    "@types/react-virtualized": "^9.21.4",
    "antd": "^3.21.4",
    "async-task-throttle": "^1.0.0",
    "babel-plugin-import": "^1.11.0",
    "concurrently": "^4.1.1",
    "coveralls": "^3.0.5",
    "cross-env": "^5.2.0",
    "customize-cra": "^0.2.11",
    "cz-conventional-changelog": "3.0.2",
    "electron": "^6.0.2",
    "electron-builder": "^21.1.5",
    "electron-devtools-installer": "^2.2.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "husky": "^3.0.3",
    "immutable": "^4.0.0-rc.12",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "pouchdb-adapter-memory": "^7.1.1",
    "pouchdb-browser": "^7.1.1",
    "pouchdb-find": "^7.1.1",
    "pretty-quick": "^1.11.1",
    "react": "^16.8.6",
    "react-app-rewired": "^2.1.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.8.6",
    "react-intl": "^3.1.8",
    "react-redux": "^7.1.0",
    "react-scripts": "3.1.1",
    "react-virtualized": "^9.21.0",
    "redux": "^4.0.4",
    "redux-saga": "^1.0.5",
    "standard-version": "^7.0.0",
    "ts-loader": "^6.0.4",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.0.0",
    "typescript": "3.5.3",
    "wait-on": "^3.3.0",
    "webpack-cli": "^3.3.6"
  }
}
